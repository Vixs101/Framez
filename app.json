{
  "expo": {
    "name": "Framez",
    "slug": "framez-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#9333ea"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.framez"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#9333ea"
      },
      "package": "com.vixs.framez",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Framez to access your photos to share posts."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5a9c2603-23de-4fad-9423-7bb105f6b639"
      }
    }
  }
}
